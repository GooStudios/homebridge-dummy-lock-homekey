{
  "pluginAlias": "VirtualHomekeyLock",
  "pluginType": "accessory",
  "schema": {
    "type": "object",
    "properties": {
      "name": {
        "title": "Name",
        "type": "string",
        "required": true,
        "default": "Virtual HomeKey Lock"
      },
      "serialNumber": {
        "title": "NFC Tag Serial Number",
        "type": "string",
        "required": true,
        "default": "123456789"
      },
      "keyColor": {
        "title": "HomeKey Color",
        "type": "string",
        "required": true,
        "default": "Silver",
        "oneOf": [
          { "title": "Tan", "const": "Tan" },
          { "title": "Gold", "const": "Gold" },
          { "title": "Silver", "const": "Silver" },
          { "title": "Black", "const": "Black" }
        ]
      }
    }
  }
}
